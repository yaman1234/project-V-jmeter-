          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="POST_Response.json" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper
import groovy.json.JsonBuilder
import org.apache.commons.io.FileUtils

// Path to the &quot;Get.json&quot; file
def filePath = &quot;D:/apache-jmeter-5.5/bin/project-V jmeter/files/GET_Response.json&quot;;

// Read the content of the &quot;Get.json&quot; file
def jsonData = new File(filePath).text

// Parse JSON data
def data = new JsonSlurper().parseText(jsonData)

// Modify JSON data
data.each { item -&gt;
    if (item[&apos;userId&apos;] % 2 != 0 &amp;&amp; item[&apos;id&apos;] % 2 == 0) {
        item[&apos;completed&apos;] = !item[&apos;completed&apos;]
    }
}

// Convert modified JSON back to string
def modifiedJson = new JsonBuilder(data).toPrettyString()

// Path to save the new JSON file
def newFilePath = &quot;D:/apache-jmeter-5.5/bin/project-V jmeter/files/POST_Response.json&quot;;

// Write modified JSON data to file
FileUtils.writeStringToFile(new File(newFilePath), modifiedJson, &quot;UTF-8&quot;)

log.info(&quot;Modified JSON data saved to file: ${newFilePath}&quot;)</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
